{
  "$schema": "http://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "lib": ["es2018", "DOM"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noEmit": true,
    "jsx": "react",
    "target": "es2015"
  }
}
